{
  "name": "iwrite-server",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "author": "Alexandra <andracolexacareer@gmail.com>",
  "license": "MIT",
  "scripts": {
    "migrate": " sequelize db:migrate",
    "migrate:refresh": "yarn run migrate:reset && yarn run migrate",
    "migrate:reset": "sequelize db:migrate:undo:all",
    "migrate:undo": "yarn run sequelize -- db:migrate:undo",
    "dev": "nodemon src/server --exec babel-node -e js",
    "start": "yarn run build && nodemon dist/index.js",
    "start:dev": "nodemon --exec yarn run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "apollo-server": "^2.21.1",
    "app-root-path": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.5.1",
    "sequelize-cli": "^6.2.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.10",
    "@babel/register": "^7.13.8",
    "nodemon": "^2.0.7"
  }
}
